{
    "name": "moo-rules",
    "version": "0.0.1",
    "description": "A digitized version of the ancient scrolls for the game of Moo.",
    "private": true,
    "scripts": {
        "dev": "concurrently -n NEXT,TS -c magenta,cyan \"yarn serve\" \"yarn ts-watch\"",
        "serve": "next dev -p 1000",
        "ts-watch": "tsc --watch --noEmit --incremental --preserveWatchOutput --pretty",
        "build": "next build && next export",
        "deploy": "npm run build & deploy.cmd"
    },
    "repository": {},
    "keywords": [],
    "author": "Phil Bladen",
    "license": "ISC",
    "devDependencies": {
        "@types/node": "18.8.3",
        "@types/react": "^18.0.21",
        "autoprefixer": "^10.4.12",
        "postcss": "^8.4.17",
        "sass": "^1.50.0",
        "tailwindcss": "^3.1.8",
        "typescript": "^4.6.3"
    },
    "dependencies": {
        "@heroicons/react": "^2.0.12",
        "clsx": "^1.2.1",
        "next": "^12.3.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    }
}
